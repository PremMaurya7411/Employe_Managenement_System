{
  "name": "ems-root",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "npm-run-all -p dev:frontend dev:server",
    "dev:frontend": "npm run dev --prefix frontend",
    "dev:server": "npm run start --prefix server",
    "prod": "npm-run-all build:frontend start:server",
    "build:frontend": "npm run build --prefix frontend",
    "start:server": "NODE_ENV=production node server/index.js"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
